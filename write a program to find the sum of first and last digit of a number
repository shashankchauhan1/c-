// write a program to find the sum of first and last digit of a number



#include<iostream>
#include<string>
using namespace std;

int main() {
	// your code goes here
	int c;
	cin>>c;
	while(c>0){
	    int x;
	    cin>>x;
	    int n2=x%10;
	    
	    string s=to_string(x);
	    int n1=s[0]-'0';
	    cout<<n1+n2<<endl;
	    c--;
	}
	
	return 0;
}
