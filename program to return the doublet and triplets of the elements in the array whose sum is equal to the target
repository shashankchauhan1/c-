// program to return the doublet and triplets of the elements in the array whose sum is equal to the target.

1> doublet

#include<iostream>
#include<vector>
using namespace std;

int main(){
    int n,target;
    cin>>n>>target;
    vector<int>v(n);
    for(int i=0; i<n; i++){
        cin>>v[i];
    }
    
    for(int i=0; i<n; i++){
        for(int j=1; j<n; j++){
            if(v[i]+v[j]==target){
                cout<<"["<<v[i]<<","<<v[j]<<"]"<<endl;
            }
        }
    }
    
    return 0;
}
