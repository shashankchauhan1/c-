// write a program to create a class to take input from user and print it.
  

#include<iostream>
using namespace std;

class A{
    private:
        int n;
    public:
        void setValue();
        void getValue();
};

void A::setValue(){
    cin>>n;
}

void A::getValue(){
    cout<<n;
}

int main(){
    A obj1;
    obj1.setValue();
    obj1.getValue();
    return 0;
}
