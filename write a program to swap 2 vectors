// write a program to swap 2 vectors



#include<iostream>
#include<vector>
using namespace std;

int main(){
    vector<int> v(4,20);
    for(int el:v){
        cout<<el<<' ';
    }
    cout<<endl;

    vector<int> b(4,10);
    for(int el:b){
        cout<<el<<' ';
    }

    swap(v,b);
    
    cout<<endl;
    for(int el:v){
        cout<<el<<' ';
    }
    
    cout<<endl;
    for(int el:b){
        cout<<el<<' ';
    }
    
    return 0;
}
