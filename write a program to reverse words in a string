// write a program to reverse words in a string 


// creating a new string

#include<iostream>
#include<algorithm>
using namespace std;

int main(){
    string str;
    string newstr="";
    string ns="";
    getline(cin,str);
    reverse(str.begin(),str.end());
    cout<<str<<endl;
    for(int i=0; i<str.length(); i++){
        if(str[i]==' '){
            reverse(newstr.begin(),newstr.end());
            ns+=newstr;
            newstr="";
        }
        else {
            newstr+=str[i];
        }
    }
    reverse(newstr.begin(),newstr.end());
    ns+=newstr;
    cout<<ns<<' ';
    return 0;
}




// change in the same string


#include<iostream>
#include<algorithm>
using namespace std;

int main(){
    string str;
    getline(cin,str);
    reverse(str.begin(),str.end());
    int start=0;
    for(int i=0; i<str.length(); i++){
        if(str[i]==' '){
            reverse(str.begin()+start,str.begin()+i);
            start=i+1;
        }
    }
    
    reverse(str.begin()+start,str.end());
    cout<<str;
    
    return 0;
}
