// write a program to find the sum of the array elements


#include<iostream>
using namespace std;
int main(){
    int sum=0;
    int n;
    cout<<"Enter size of the array: ";
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    for(int i=0; i<n; i++){
        sum+=arr[i];
    }
    cout<<"sum of the elements are: "<<sum<<endl;
    return 0;
}
