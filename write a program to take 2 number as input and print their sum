/// write a program to take 2 number as input and print their sum


#include<iostream>
using namespace std;

int main(){
    int marks1;
    int marks2;
    int sum;
    
    cout<<"Enter marks 1: ";
    cin>>marks1;
    
    cout<<"Enter marks 2: ";
    cin>>marks2;
    
    sum=marks1+marks2;
    
    cout<<"SUM is : "<<sum<<endl;
    return 0;
}
