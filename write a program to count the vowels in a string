// write a program to count the vowels in a string


#include<iostream>
using namespace std;

int main(){
    int count=0;
    string str;
    cin>>str;
    for(int i=0; str[i]!='\0'; i++){
        if(str[i]=='a'|| str[i]=='e'|| str[i]=='i'|| str[i]=='o'|| str[i]=='u'|| str[i]=='A'|| str[i]=='E'|| str[i]=='I'|| str[i]=='O'|| str[i]=='U'){
            count++;
        }
    }
    cout<<"Vowels are: "<<count<<endl;
    return 0;
}
