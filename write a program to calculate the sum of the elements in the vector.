// write a program to calculate the sum of the elements in the vector.


#include<iostream>
#include<vector>
using namespace std;

int main(){
    int n,sum=0;
    cout<<"Enter the size of the vector: ";
    cin>>n;
    vector<int> v;
    cout<<"Enter array elements: ";
    for(int i=0; i<n; i++){
        int num=0;
        cin>>num;
        v.push_back(num);
    }
    
    for(int i=0; i<n; i++){
        sum+=v[i];
    }
    cout<<"sum of the elements are: "<<sum;
    return 0;
}
