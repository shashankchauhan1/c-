//sum of 2D array elements




#include<iostream>
using namespace std;

int main(){
    int sum=0;
    int a,b;
    cout<<"Array rows and columns: ";
    cin>>a>>b;
    int arr[a][b];
    for(int i=0; i<a; i++){
        for(int j=0; j<b; j++){
            cin>>arr[i][j];
        }
    }
    for(int i=0; i<a; i++){
        for(int j=0; j<b; j++){
            sum+=arr[i][j];
        }
    }
    cout<<sum;
    return 0;
}
