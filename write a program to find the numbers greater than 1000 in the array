// write a program to find the numbers greater than 1000 in the array.

#include<iostream>
using namespace std;
int main() {
	int c;
	cin>>c;
	while(c>0){
	   
	   int n;
	   cin>>n;
	   int arr[n];
	   for(int i=0; i<n; i++){
	       cin>>arr[i];
	   }
	   int count=0;
	   
	   for(int i=0; i<n; i++){
	       if(arr[i]>=1000){
	           count++;
	       }	           
	   }
	    cout<<count<<endl;
	    c--;
	}
	return 0;
}
