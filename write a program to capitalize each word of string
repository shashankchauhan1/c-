// write a program to capitalize each word of string


#include<iostream>
#include<string>
using namespace std;

int main(){
    
    string str;
    getline(cin,str);
    str[0]=toupper(str[0]);
    for(int i=0; str[i]!='\0'; i++){
        if(str[i]==' '){
            str[i+1]=toupper(str[i+1]);
        }
    }
    cout<<str;
    
    return 0;
}
