// write a program to find the square of each elements in the array

#include<iostream>
using namespace std;
int main(){
    int sum=1;
    int n;
    cout<<"Enter size of the array: ";
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    for(int i=0; i<n; i++){
        arr[i]*=arr[i];
    }

    cout<<"square of the elements are: "<<endl;
    for(int i=0; i<n; i++){
        cout<<arr[i]<<' ';
    }
    return 0;
}
