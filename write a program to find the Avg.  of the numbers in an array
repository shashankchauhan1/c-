// write a program to find the Avg.  of the numbers in an array

#include <iostream>
using namespace std;

int main(){
    int arr[10]={91,82,73,64,75};
    int n=5;
    int sum,avg;
    for(int i=0; i<n; i++){
        sum+=arr[i];
    }
    avg=sum/n;
    cout<<"Average: "<<avg<<endl;
    return 0;
}
