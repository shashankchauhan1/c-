// write a program to find the greatest among 3 numbers.


#include<iostream>
using namespace std;

int main(){
    int a,b,c;
    cout<<"Enter a: ";
    cin>>a;
    
    cout<<"Enter b: ";
    cin>>b;
    
    cout<<"Enter c: ";
    cin>>c;
    
    if(a>b && a>c){
        cout<<"a is greater"<<endl;
    }
    else if(b>c && b>a){
        cout<<"b is greater"<<endl;
    }
    else{
        cout<<"c is greater"<<endl;
    }
    return 0;
}
