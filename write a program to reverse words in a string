// write a program to reverse words in a string 


#include<iostream>
#include<algorithm>
using namespace std;

int main(){
    string str;
    string newstr="";
    string ns="";
    getline(cin,str);
    reverse(str.begin(),str.end());
    cout<<str<<endl;
    for(int i=0; i<str.length(); i++){
        if(str[i]==' '){
            reverse(newstr.begin(),newstr.end());
            ns+=newstr;
            newstr="";
        }
        else {
            newstr+=str[i];
        }
    }
    reverse(newstr.begin(),newstr.end());
    ns+=newstr;
    cout<<ns<<' ';
    return 0;
}
